{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_API_BASE_URL": "https://snipswap-dex-production.up.railway.app",
    "VITE_API_MARKET_ENDPOINT": "/api/market",
    "VITE_API_TRADING_ENDPOINT": "/api/trading",
    "VITE_API_AUTH_ENDPOINT": "/api/auth",
    "VITE_API_PRIVATE_ENDPOINT": "/api/private",
    "VITE_WS_URL": "wss://snipswap-dex-production.up.railway.app",
    "VITE_APP_NAME": "SnipSwap DEX",
    "VITE_APP_VERSION": "3.0.0",
    "VITE_ENVIRONMENT": "production",
    "VITE_PRIVACY_ENABLED": "true",
    "VITE_SOVEREIGNTY_STACK": "true"
  }
}

